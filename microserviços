server {
        listen 8001;
        server_name localhost;
        access_log https://github.com/marcosf-cruz1980/NGINX/blob/main/logs/servicos1.log main;

        location / {
            root https://github.com/marcosf-cruz1980/NGINX/tree/main/www/servidor1;
            index index.html;
        }
}

